{
    "liveServer.settings.port": 12346,

    "files.exclude": {
        "node_modules": true
    },
    "editor.minimap.enabled": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "package.json": "package-lock.json, .gitignore",
        "README.md": "*.md, *.ico, LICENSE, LICENSE-free.txt",
        "index.html": "*.html",
        "aos.css": "*.css",
        "*bootstrap.min.js": "*.js.map, *.min.js, *.d.ts",
        "bootstrap-datepicker.js":  "*.js.map, *.min.js, *.d.ts, *.js",
        "aos.js": "*.js.map, *.min.js, *.d.ts, *.js",
        "favicon_16.ico": "*.ico",
        "cc-bg-1.jpg": "*.jpg, *.svg, *.png, *.jpeg",
        "c_basic.png": "*.jpg, *.svg, *.png, *.jpeg",
        "Jadavpur_University_Logo.png": "*.jpg, *.svg, *.png, *.jpeg",
        "3utools.png": "*.jpg, *.svg, *.png, *.jpeg",
        "mediakind.svg":  "*.jpg, *.svg, *.png, *.jpeg",
        "compose.yaml": ".docker*"
    }
}